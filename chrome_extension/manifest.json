{
  "name": "Airtab",
  "version": "1.0",
  "description": "Control your HTPC from chrome",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "http://*/*"
  ],
  "icons": {
    "16": "icon.png"
  },
  "options_page": "options.html",
  "background_page": "background.html"
}
